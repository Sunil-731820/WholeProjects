package com.java.json;

class GfG{
	
	int gfgId;
	String userName;
	char gender;
	public GfG(int gfgId, String userName, char gender) {
		super();
		this.gfgId = 0;
		this.userName = "";
		this.gender = ' ';
	}
	
	
}

public class JsonToObject {
	public static void main(String[] args) {
		
		GfG gfg = null;
		String jsonString ;
		jsonString = "{";
		jsonString +="gfgId : 10001";
		jsonString +="userName : 'jackson '";
		jsonString +="gender : 'M'";
		jsonString +="}";
		
//		Creating object of the json 
	}

}
